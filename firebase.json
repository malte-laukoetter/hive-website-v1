{
  "hosting": {
    "public": "build/default",
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [ 
      {
        "source" : "service-worker.js",
        "headers" : [ 
          {
            "key" : "Cache-Control",
            "value" : "max-age=300"
          } 
        ]
      }
    ]
  }
}
